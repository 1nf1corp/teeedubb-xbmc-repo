<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="script.steam.launcher" name="Steam" version="1.9.4" provider-name="teeedubb">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.script" library="default.py">
        <provides>game executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Launches Steam in Big Picture mode from within XBMC.</summary>
        <description lang="en">Launches Steam from within XBMC - It will close or minimize XBMC, launch Steam in Big
            Picture mode and when Steam is exited XBMC will restart or maximize. Steam also comes with a full featured
            web browser than can be controlled with a game pad or remote and plays back flash content. It works with
            Windows, Linux, OSX and Android. Each OS has different settings available.
        </description>
        <platform>linux windx osx android</platform>
        <language>en</language>
        <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
        <forum>http://forum.xbmc.org/showthread.php?tid=157499</forum>
        <website>http://forum.xbmc.org/showthread.php?tid=157499</website>
        <source>https://github.com/teeedubb</source>
        <email></email>
    </extension>
</addon>

<addon id="script.audio.party.mode.start" name="Music Party Mode" version="0.1.4" provider-name="teeedubb">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.script" library="default.py">
     <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Starts Music Party Mode.</summary>
    <description lang="en">Starts Music Party Mode, waits for X seconds then switches to full screen visualisation and optionally toggles the fullscreen music info after X seconds. Timeouts can be configured in addon settings, defaults are 5 seconds for each.</description>
    <platform>all</platform>
    <language>en</language>
    <forum>http://forum.xbmc.org/showthread.php?tid=166842</forum>
    <website>http://forum.xbmc.org/showthread.php?tid=166842</website>
    <source>https://github.com/teeedubb</source>
    <email></email>
  </extension>
</addon>

